{
    "name": "opengento/module-country-store",
    "description": "This module add the many countries to many stores relation and make it available to the storefront.",
    "keywords": [
        "php",
        "magento",
        "magento2",
        "store",
        "website",
        "country"
    ],
    "require": {
        "php": "^7.4",
        "psr/log": "*",
        "magento/framework": "*",
        "magento/module-config": "*",
        "magento/module-customer": "*",
        "magento/module-directory": "*",
        "magento/module-store": "*",
        "magento/module-theme": "*"
    },
    "require-dev": {
        "magento/marketplace-eqp": "^1.0.0"
    },
    "suggest": {
        "opengento/module-country-store-redirect": "This module will redirect the customers regarding their country of origin, on their first visit session.",
        "opengento/module-country-store-switcher": "This module will allows the customers to switch of store by country instead of name/language.",
        "opengento/module-country-store-phone": "This module add the phone number field to the country data object."
    },
    "type": "magento2-module",
    "license": [
        "MIT"
    ],
    "homepage": "https://github.com/opengento/magento2-country-store",
    "authors": [
        {
            "name": "Opengento Team",
            "email": "opengento@gmail.com",
            "homepage": "https://opengento.fr/",
            "role": "lead"
        },
        {
            "name": "Thomas Klein",
            "email": "thomaskein876@gmail.com",
            "homepage": "https://www.linkedin.com/in/thomas-klein/",
            "role": "maintainer"
        }
    ],
    "support": {
        "source": "https://github.com/opengento/magento2-country-store",
        "issues": "https://github.com/opengento/magento2-country-store/issues"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Opengento\\CountryStore\\": ""
        }
    }
}
